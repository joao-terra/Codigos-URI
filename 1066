#include <stdio.h>


int main () {

int x, i, par = 0, impar = 0, posi = 0, neg = 0;

for (i = 0; i < 5; i++){

  scanf("%d", &x);
  if (x > 0){

    posi++;

  }
  if (x < 0){

    neg++;

  }
  if (x % 2 == 0){

    par++;

  }
  if (x % 2 != 0){

    impar++;

  }
}

printf("%i valor(es) par(es)\n", par);
printf("%i valor(es) impar(es)\n", impar);
printf("%i valor(es) positivo(s)\n", posi);
printf("%i valor(es) negativo(s)\n", neg);

    return 0;
}
