#include <stdio.h>

int main() {
  float  n=0,t=0,media=0;
  int quant=0,tn=0;
    
        for (quant = 1; quant <= 6; quant++) {
          scanf("%f", &n);
        if (n > 0) {
          tn += 1;
          t += n;
       }
      }
      media = t / tn;

     printf("%d valores positivos\n", tn);
     printf("%.1f\n",media);
    return 0;
}
