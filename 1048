#include <stdio.h>
int main () {

  float sal, ganha;
  scanf("%f", &sal);

  if (sal >= 0 && sal <= 400){

    ganha = sal*0.15;
    sal = sal + ganha;
    printf("Novo salario: %.2f\n", sal);
    printf("Reajuste ganho: %.2f\n", ganha);
    printf("Em percentual: 15 %%\n");

  }
  else{

      if (sal > 400 && sal <= 800){

        ganha = sal*0.12;
        sal = sal + ganha;
        printf("Novo salario: %.2f\n", sal);
        printf("Reajuste ganho: %.2f\n", ganha);
        printf("Em percentual: 12 %%\n");


        }
        else{

          if (sal > 800 && sal <= 1200){

            ganha = sal*0.1;
            sal = sal+ ganha;
            printf("Novo salario: %.2f\n", sal);
            printf("Reajuste ganho: %.2f\n", ganha);
            printf("Em percentual: 10 %%\n");

          }
          else{

              if (sal > 1200 && sal <= 2000){
              ganha = sal*0.07;
              sal = sal + ganha;
              printf("Novo salario: %.2f\n", sal);
              printf("Reajuste ganho: %.2f\n", ganha);
              printf("Em percentual: 7 %%\n");
            }
            else{

              ganha = sal*0.04;
              sal = sal + ganha;
              printf("Novo salario: %.2f\n", sal);
              printf("Reajuste ganho: %.2f\n", ganha);
              printf("Em percentual: 4 %%\n");

            }
          }
        }
      }

    return 0;
}
