#include <stdio.h>
 
int main() {
        char funcionario;
        double salario , vendas , ganha;
        scanf ("%s" , &funcionario);
        scanf ("%lf" , &salario);
        scanf ("%lf" , &vendas);
        ganha = vendas*15/100 + salario;
        printf ("TOTAL = R$ %.2lf\n", ganha);
    return 0;
}
