#include <stdio.h>
 
int main() { 
    int h1, h2;

    scanf("%i %i", &h1, &h2);
    if (h1 > h2) { 
        printf("O JOGO DUROU %i HORA(S)\n", 24 - (h1 - h2));
    } else if (h2 > h1) {
        printf("O JOGO DUROU %i HORA(S)\n", h2 - h1);
    } else {
        printf("O JOGO DUROU 24 HORA(S)\n");
    }
    
    return 0;
}
